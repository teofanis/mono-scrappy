{
    "name": "@scrappy/backend",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "vendor/bin/sail up",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "start": "vendor/bin/sail up",
        "setup": "composer install && vendor/bin/sail up && sail php artisan scrappy-setup"
    },
    "devDependencies": {
        "axios": "^0.21",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.1.14"
    }
}
